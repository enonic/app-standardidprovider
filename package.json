{
  "name": "app-standardidprovider",
  "version": "1.0.0",
  "description": "Standard Id provider application for Enonic XP Admin",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/enonic/app-standardidprovider"
  },
  "scripts": {
    "build": "concurrently -c auto -g --timings npm:build:*",
    "build:server": "node tsup/anyServerFiles.js && npx tsup -d build/resources/main || exit 0",
    "build:static": "webpack --color",
    "check:types": "concurrently -g -r --timings npm:check:types:*",
    "check:types:browser": "node tsup/anyAssetFiles.js && npx tsc --noEmit -p /tsconfig.browser.json || exit 0",
    "check:types:xp": "node tsup/anyServerFiles.js && npx tsc --noEmit -p tsconfig.xp.json || exit 0",
    "lint": "eslint .",
    "fix": "eslint . --fix"
  },
  "devDependencies": {
    "@enonic-types/global": "^7.14.4",
    "@enonic-types/lib-admin": "^7.14.4",
    "@enonic-types/lib-auth": "^7.14.4",
    "@enonic-types/lib-context": "^7.14.4",
    "@enonic-types/lib-node": "^7.14.4",
    "@enonic-types/lib-portal": "^7.14.4",
    "@item-enonic-types/lib-mustache": "^2.1.0",
    "@item-enonic-types/lib-static": "^1.0.2",
    "@swc/core": "^1.7.26",
    "autoprefixer": "^10.4.20",
    "browserslist": "^4.24.0",
    "browserslist-config-enonic": "^1.0.8",
    "concurrently": "^9.0.1",
    "css-loader": "^7.1.2",
    "cssnano": "^7.0.6",
    "enonic-admin-artifacts": "^2.2.0",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-xo": "^0.45.0",
    "eslint-plugin-prettier": "^5.2.1",
    "image-minimizer-webpack-plugin": "^4.1.0",
    "less": "^4.2.0",
    "less-loader": "^12.2.0",
    "mini-css-extract-plugin": "^2.9.1",
    "postcss": "^8.4.47",
    "postcss-loader": "^8.1.1",
    "postcss-normalize": "^13.0.1",
    "postcss-sort-media-queries": "^5.2.0",
    "prettier": "^3.3.3",
    "sharp": "^0.33.5",
    "terser-webpack-plugin": "^5.3.10",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@enonic/js-utils": "^1.8.0",
    "jquery": "^3.7.1"
  },
  "browserslist": [
    "extends browserslist-config-enonic"
  ],
  "engines": {
    "node": ">= 20.13.1",
    "npm": ">= 10.5.2"
  }
}
